{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@craco/craco": "^6.4.5",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.10",
    "craco-swc": "^0.5.1",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.1",
  "dependencies": {
    "@swc/plugin-emotion": "^2.5.17",
    "emotion-ts-plugin": "^2.0.1",
    "vite-tsconfig-paths": "^3.5.2"
  }
}
